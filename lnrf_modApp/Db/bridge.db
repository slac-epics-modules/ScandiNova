record (stringin,"$(P):MainEventTimeStr") {
	field(DESC, "Timestamp of the last main event in mod")
}

record (stringin,"$(P):MainEventTypeStr") {
	field(DESC, "Event type of the last main event in mod")
}

record (stringin,"$(P):MainEventTrigger") {
	field(DESC, "Trigger ID of the last main event in mod")
}

record (stringin,"$(P):MainEventTextStr") {
	field(DESC, "Text explanation of the last main event")
}

record (stringin,"$(P):MainEventTextStr2") {
	field(DESC, "Second half of MainEventTextStr")
}

record (stringin,"$(P):MainEventTextNumStr") {
	field(DESC, "State of the last main event")
}

record (stringin,"$(P):LastIntlkTypeStr") {
	field(DESC, "Event type of the last interlock event")
}

record (stringin,"$(P):LastIntlkTimeStr") {
	field(DESC, "Timestamp of the last interlock event")
}

record (stringin,"$(P):LastIntlkTrigger") {
	field(DESC, "Trigger ID of the last interlock event")
}

record (stringin,"$(P):LastIntlkTextStr") {
	field(DESC, "Text explanation of last interlock event")
}

record (stringin,"$(P):LastIntlkTextStr2") {
	field(DESC, "Second half of LastIntlkTextStr")
}

record (aSub,"$(P):EventBridge") {
	field(SNAM,"handle_event_modify")

	field(INPA,"$(P):EventIndex CP")
	field(FTA,"LONG")
	field(NOA,"1")

	field(INPB,"$(P):EventIncrement CP")
	field(FTB,"LONG")
	field(NOB,"50")

	field(INPC,"$(P):EventTime1 CP")
	field(FTC,"LONG")
	field(NOC,"50")
	field(OUTC,"$(P):MainEventTimeStr PP")
	field(FTVC,"STRING")
	field(NOVC,"64")

	field(INPD,"$(P):EventTime2 CP")
	field(FTD,"LONG")
	field(NOD,"50")

	field(INPE,"$(P):EventType CP")
	field(FTE,"LONG")
	field(NOE,"50")
	field(OUTE,"$(P):MainEventTypeStr PP")
	field(FTVE,"STRING")
	field(NOVE,"32")

	field(INPF,"$(P):EventTrigger CP")
	field(FTF,"LONG")
	field(NOF,"50")
	field(OUTF,"$(P):MainEventTrigger PP")
	field(FTVF,"LONG")
	field(NOVF,"1")

	field(INPG,"$(P):EventCause CP")
	field(FTG,"LONG")
	field(NOG,"50")
	field(OUTG,"$(P):MainEventTextStr PP")
	field(FTVG,"STRING")
	field(NOVG,"64")

	field(INPH,"$(P):EventTextNum CP")
	field(FTH,"LONG")
	field(NOH,"50")
	field(OUTH, "$(P):MainEventTextNumStr PP")
	field(FTVH,"STRING")
	field(NOVH,"64")

	field(INPI,"$(P):EventDataType CP")
	field(FTI,"LONG")
	field(NOI,"50")

	field(INPJ,"$(P):EventData CP")
	field(FTJ,"LONG")
	field(NOJ,"50")

	field(OUTK,"$(P):MainEventTextStr2 PP")
	field(FTVK,"STRING")
	field(NOVK,"64")

	field(OUTL,"$(P):LastIntlkTypeStr PP")
	field(FTVL,"STRING")
	field(NOVL,"32")

	field(OUTM,"$(P):LastIntlkTimeStr PP")
	field(FTVM,"STRING")
	field(NOVM,"64")

	field(OUTN,"$(P):LastIntlkTrigger PP")
	field(FTVN,"LONG")
	field(NOVN,"1")

	field(OUTO,"$(P):LastIntlkTextStr PP")
	field(FTVO,"STRING")
	field(NOVO,"64")

	field(OUTP,"$(P):LastIntlkTextStr2 PP")
	field(FTVP,"STRING")
	field(NOVP,"64")
}
