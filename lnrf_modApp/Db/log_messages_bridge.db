record (aSub,"$(P):CurrentEventStructBridge") {
	field(SNAM,"handle_current_event_struct_modify")

# input current event struct

	field(INPA,"$(P):CurrentEventStruct CP")
	field(FTA,"LONG")
	field(NOA,"13")

# output current event

	field(OUTA,"$(P):CurrentEventTimeStr PP")
	field(FTVA,"STRING")
	field(NOVA,"64")

	field(OUTB,"$(P):CurrentEventIocTimeStr PP")
	field(FTVB,"STRING")
	field(NOVB,"64")

	field(OUTC,"$(P):CurrentEventTypeStr PP")
	field(FTVC,"STRING")
	field(NOVC,"64")

	field(OUTD,"$(P):CurrentEventTrigger PP")
	field(FTVD,"LONG")
	field(NOVD,"1")

	field(OUTE,"$(P):CurrentEventSubsystemStr PP")
	field(FTVE,"STRING")
	field(NOVE,"64")

	field(OUTF,"$(P):CurrentEventTextStr PP")
	field(FTVF,"STRING")
	field(NOVF,"64")

	field(OUTG,"$(P):CurrentEventUnitStr PP")
	field(FTVG,"STRING")
	field(NOVG,"64")

	field(OUTH,"$(P):CurrentEventDataStr PP")
	field(FTVH,"STRING")
	field(NOVH,"64")

# output warning event

	field(OUTI,"$(P):WarningEventTimeStr PP")
	field(FTVI,"STRING")
	field(NOVI,"64")

	field(OUTJ,"$(P):WarningEventIocTimeStr PP")
	field(FTVJ,"STRING")
	field(NOVJ,"64")

	field(OUTK,"$(P):WarningEventTypeStr PP")
	field(FTVK,"STRING")
	field(NOVK,"64")

	field(OUTL,"$(P):WarningEventTrigger PP")
	field(FTVL,"LONG")
	field(NOVL,"1")

	field(OUTM,"$(P):WarningEventSubsystemStr PP")
	field(FTVM,"STRING")
	field(NOVM,"64")

	field(OUTN,"$(P):WarningEventTextStr PP")
	field(FTVN,"STRING")
	field(NOVN,"64")

	field(OUTO,"$(P):WarningEventUnitStr PP")
	field(FTVO,"STRING")
	field(NOVO,"64")

	field(OUTP,"$(P):WarningEventDataStr PP")
	field(FTVP,"STRING")
	field(NOVP,"64")
}

